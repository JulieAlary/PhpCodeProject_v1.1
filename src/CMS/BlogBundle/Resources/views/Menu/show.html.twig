{% extends 'CMSBlogBundle::layout.html.twig' %}



{% block cms_body %}
    <ul>

        {% for article in listArticles %}
            {% for a in article.categories %}
                {% for m in listM %}
                    {% if m.published == true %}

                        {% for i in m.categories %}

                            {% if a.id == id %}
                                {{ article.title }}
                                {{ article.content|raw }}

                            {% endif %}
                        {% endfor %}
                    {% endif %}
                {% endfor %}
            {% endfor %}
        {% endfor %}
    </ul>


{% endblock %}

